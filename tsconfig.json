{
    "compilerOptions": {
      "allowJs": true,
      "module": "ESNext",
      "moduleResolution": "bundler",
      "jsx": "react-jsx",
      "strict": true,
      "isolatedModules": true,
      "target": "ESNext",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "noEmit": true,
      "skipLibCheck": true,
      "types": ["vite/client"],
      "baseUrl": ".",
      "paths": {
        "@/*": ["./resources/js/*"],
      }
    },
    "include": [
      "resources/js/**/*.ts",
      "resources/js/**/*.tsx",
      "resources/js/**/*.d.ts",
      "resources/js/vite-env.d.ts"
    ]
  }